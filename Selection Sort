#include <iostream>
using namespace std;
int arr[10] = { 5,5,6,8,9,1,2,3,0,1 };
void swap(int &x, int &y)
{
	int temp = x;
	x = y;
	y = temp;
}
void sort_selection(int n)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = i + 1; j < n; j++)
		{
			if (arr[i] > arr[j])
				swap(arr[i], arr[j]);
		}
	}
}
void print_array(int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << " ";
	}
}
int main()
{
	sort_selection(10);
	print_array(10);

}
