#include <iostream>
using namespace std;
int arr[10] = { 5,5,6,8,9,1,2,3,0,1 };
void swap(int &x, int &y)
{
	int temp = x;
	x = y;
	y = temp;
}

void bubbly_sort(int n)
{
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n-i; j++)
		{
			if (arr[j] > arr[j + 1])
				swap(arr[j], arr[j + 1]);
		}
	}
}
void print_array(int n)
{
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << " ";
	}
}
int main()
{
	bubbly_sort(10);
	print_array(10);

}
